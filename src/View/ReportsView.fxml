      <TextArea fx:id="TextAreaOutput" layoutX="17.0" layoutY="62.0" prefHeight="317.0" prefWidth="558.0" />
      <MenuButton fx:id="menuReports" layoutX="86.0" layoutY="22.0" mnemonicParsing="false" text="Reports">
