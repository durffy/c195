          <MenuItem fx:id="MenuReportsItemNumberOfAppointmentsByType" mnemonicParsing="false" onAction="#clickMenuReportsItemNumberOfAppointmentsByType" text="Number of Appointments by Type" />
          <MenuItem fx:id="MenuReportsItemSchedulePerConsultant" mnemonicParsing="false" onAction="#clickMenuReportsItemSchedulePerConsultant" text="Schedule Per Consultant" />
            <MenuItem fx:id="MenuReportsItemActiveCustomers" mnemonicParsing="false" onAction="#clickMenuReportsItemActiveCustomers" text="Active Customers" />
        </items>
      </MenuButton>
      <Button fx:id="ButtonGoBack" layoutX="17.0" layoutY="22.0" mnemonicParsing="false" onAction="#clickButtonGoBack" text="Go Back" />
   </children>
</AnchorPane>
